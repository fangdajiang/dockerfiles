{
  "inbounds": [{
    "port": 10000,
    "listen":"0.0.0.0",
    "protocol": "vmess",
    "settings": {
      "clients": [
        $UUID_LIST
      ]
    },
    "streamSettings": {
      "network": "ws",
      "wsSettings": {
        "path": "$PATH_RAY"
      }
    }
  }],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  },{
    "protocol": "blackhole",
    "settings": {},
    "tag": "blocked"
  }],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "blocked"
      }
    ]
  }
}
